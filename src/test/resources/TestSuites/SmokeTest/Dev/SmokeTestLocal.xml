<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ENE Test Suite" thread-count="2" parallel="false">
		<!-- This parameter used to select running local or grid (remote)-->
		<parameter name="environment" value="local"></parameter>
		<!-- This parameter to select the environment where to run test,dev,preprod-->
		<parameter name="testEnvironment" value="prod"></parameter>
		
		<test name="Smoke-Test">
			<parameter name="browser" value="firefox"></parameter>
			<packages>
				<package name="smokeTest" />
			</packages>
		</test>

</suite> <!-- Suite -->


